{
  "version": 1,
  "projects": {
    "play": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/play",
      "sourceRoot": "projects/play/src",
      "prefix": "wx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/play",
            "index": "projects/play/src/index.html",
            "main": "projects/play/src/main.ts",
            "polyfills": "projects/play/src/polyfills.ts",
            "tsConfig": "projects/play/tsconfig.app.json",
            "aot": true,
            "assets": ["projects/play/src/favicon.ico", "projects/play/src/assets"],
            "styles": ["projects/play/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/play/src/environments/environment.ts",
                  "with": "projects/play/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "play:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "play:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "play:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/play/tsconfig.app.json",
              "projects/play/tsconfig.spec.json",
              "projects/play/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@wdtk/jest:jest",
          "options": {
            "jestConfig": "projects/play/jest.config.js",
            "tsConfig": "projects/play/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "projects/play/src/test-setup.ts"
          }
        },
        "e2e": {
          "builder": "@wdtk/cypress:cypress",
          "options": {
            "cypressConfig": "projects/play/cypress.json",
            "tsConfig": "projects/play/e2e/tsconfig.json",
            "devServerTarget": "play:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "play:serve:production"
            }
          }
        }
      }
    }
  },
  "newProjectRoot": "projects",
  "cli": {
    "defaultCollection": "@wdtk/workspace",
    "packageManager": "yarn"
  },
  "defaultPrefix": "wx",
  "natures": {
    "@wdtk/angular": {
      "name": "Angular"
    }
  },
  "defaultProject": "play"
}
